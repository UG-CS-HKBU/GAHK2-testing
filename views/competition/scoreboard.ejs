<div class="container">
    <div class="row">
        <div class="card col-12">
            <div class="card-body col-12">

                <div>
                    <h2>
                        <b><%=competition.competitionEvent%></b>
                    </h2>
                    <h2>
                        <b></b>
                    </h2>
                </div>

                <%  
      
                    <!-- model.dAvgScore = (model.d1Score + model.d2Score) / 2 -->
                    sortEScore = [competition.e1Score, competition.e2Score, competition.e3Score, competition.e4Score, competition.e5Score].sort();
                    competition.eAvgScore = 10 - (sortEScore[1] + sortEScore[2] + sortEScore[3]) / 3;
                    competition.totalScore = competition.d1Score + competition.eAvgScore - competition.deduction;
          
                    temp = competition.totalScore.toString(); 
                    temp = temp.slice(0, (temp.indexOf("."))+4); 
                    competition.totalScore = Number(temp);
                     
                    
                    %>


                <div class="media">
                       

                    <div class="media-body">
                        

                        <div class="card" style="width: 18rem;">
                         

                            <ul class="list-group">

                                <li class="list-group-item"><b>Athlete Name: </b><%= competition.athleteName %></li>
                                <li class="list-group-item"><b>Difficulty: </b><%= competition.d1Score %></li>
                                <li class="list-group-item"><b>Execution: </b><%= competition.eAvgScore %></li>
                                <li class="list-group-item"><b>Extra Deduction: </b><%= competition.deduction %></li>
                                <li class="list-group-item"><b>Final Score: </b><%= competition.totalScore %></li>



                            </ul>
                        </div>


                    </div>
                </div>
                <br>



                <!--add-->


            </div>
        </div>


    </div>
</div>

<br>